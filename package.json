{
  "name": "electron-playground",
  "version": "1.0.1",
  "description": "Learning Electron with React and TypeScript",
  "main": "./dist/main.bundle.js",
  "scripts": {
    "start": "electron ./dist/main.bundle.js",
    "debug": "rimraf dist && webpack --config ./webpack.dev.js --progress --color && electron ./dist/main.bundle.js",
    "build": "rimraf dist && webpack --config ./webpack.dev.js --progress --color",
    "production": "rimraf dist && webpack --config ./webpack.prod.js --progress --color"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agearhart/electron-playground.git"
  },
  "keywords": [
    "electron",
    "typescript",
    "react",
    "javascript"
  ],
  "author": "Andy Gearhart",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/agearhart/electron-playground/issues"
  },
  "homepage": "https://github.com/agearhart/electron-playground#readme",
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "axios": "^0.21.1",
    "css-loader": "^5.2.6",
    "electron": ">=13.1.5",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^27.0.6",
    "node-sass": "^6.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.0.0",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.5",
    "webpack": "^5.42.0",
    "webpack-cli": "^4.7.2"
  }
}
